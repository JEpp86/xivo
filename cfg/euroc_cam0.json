{
  // verbose
  "simulation": false,
  "print_timing": false,
  "use_canvas": true,
  "use_debug_view": false,  // draw rejected & dropped features on canvas
  "async_run": false, // turn this off in benchmarking

  // visualization (tracker view) option
  "print_bias_info": true,
  "draw_OOS": true,

  // algorithmic-level knobs
  "integration_method": "PrinceDormand", // "PrinceDormand", "RK4", //, Fehlberg
  "use_OOS": false, // update with Out-Of-State features
  "use_depth_opt": false,  // depth optimization
  "use_MH_gating": false,
  "use_1pt_RANSAC": false,
  "use_compression": false,  // measurement compression
  "triangulate_pre_subfilter": true,
  "compression_trigger_ratio": 1.5,
  "remove_outlier_counter": 10,
  "OOS_update_min_observations": 5,
  "feature_owner_change_cov_factor": 1.5,

  "PrinceDormand": {
    "control_stepsize": false,
    "tolerance": 1e-15,
    "attempts": 12,
    "min_scale_factor": 0.125,
    "max_scale_factor": 4.0,
    "stepsize": 0.002
  },

  "RK4": {
    "stepsize": 0.002
  },

  // memory
  "memory": {
    "max_features": 30000,
    "max_groups": 5000  // max groups should increase as allowed group lifetime increases
  },

  // gravity constant
  "gravity": [0, 0, -9.8],
  // Initial State
  "X" : {
    "W"   : [0, 0, 0],
    "T"   : [0, 0, 0],
    "V"   : [0, 0, 0],
    "bg"  : [0, 0, 0],
    "ba"  : [0, 0, 0],
    "Wbc" : [[ 0.0148655429818, -0.999880929698, 0.00414029679422 ],
             [ 0.999557249008, 0.0149672133247, 0.025715529948 ],
             [-0.0257744366974, 0.00375618835797, 0.999660727178 ]],
    // wbc in tangent space: [ -0.017087,   0.023277,   1.555781 ]
    "Tbc" : [ -0.0216401454975, -0.064676986768, 0.00981073058949 ],
    "Wg"  : [0, -1.5708, 0],
    "td"  : 0
  },

  "P" : {
    "W"   : 0.0001,
    "T"   : 0.001,
    "V"   : 0.5,
    "bg"  : 1e-3,
    "ba"  : 1e-2,
    "Wbc" : 0.0001,
    "Tbc" : 0.001,
    "Wg"  : 3.01,
    "td"  : 1e-4, // 1ms
    "Cg"  : 1e-4,
    "Ca"  : 1e-4,
    "FC"  : [9, 0.9],
    "distortion": 0.01
  },

  "Qmodel" : {
    "W"   : 2.5,
    "T"   : 1.5,
    "V"   : 1.5,
    "wb"  : 0.0005,
    "ab"  : 0.005,
    "Wbc" : 0,
    "Tbc" : 0.0,
    "Wg"  : 0
  },

  // // Exact densities provided by EuRoC dataset
  // "Qimu": {
  //   "gyro": 1.6968e-04,
  //   "gyro_bias": 1.9393e-05,
  //   "accel": 2.0000e-03,
  //   "accel_bias": 3.0000e-03
  // },

  // inflated by 3 times
  "Qimu": {
    "gyro": [5.0904e-4, 5.0904e-4, 5.0904e-4],
    "gyro_bias": [5.8179e-05, 5.8179e-05, 5.8179e-05],
    "accel": [6.0e-3, 6.0e-3, 6.0e-3],
    "accel_bias": [9.0e-03, 9.0e-03, 9.0e-03]
  },


  // Feature initialization parameters
  "initial_z": 2.5, // initial subfilter depth value (meter)
  "initial_std_x": 1.0, // initial std on subfilter state (pixel)
  "initial_std_y": 1.0, // initial std on subfilter state (pixel)
  "initial_std_z": 1.0,  // initial std on subfilter state (meter)
  "max_subfilter_outlier": 1.01, // maximum outlier counter in subfilter allowed
                                 // for feature to be in main filter
  "strict_criteria_timesteps": 5, // before this many RGB frames, we will use a
                                  // looser criteria for admitting features to
                                  // the state
  "comparison_score_type": "DepthUncertainty",
 
  "visual_meas_std":  1.5, // std of visual measurement for x and y (pixel)
  "outlier_thresh": 1.1,
  "oos_meas_std": 3.5,
  "max_depth": 15.0,
  "min_depth": 0.01,

  // Adaptive initial depth
  "adaptive_initial_depth": {
    "median_weight": 0.99,
    "minimum_feature_lifetime": 5
  },

  // Loop closure measurement, in pixels
  "loop_closure_meas_std": 4.0,

  // depth sub-filter setting
  "subfilter": {
    "visual_meas_std":  3.5,
    "ready_steps": 2,
    "MH_thresh": 8.991
  },

  // pre-subfilter triangulation options
  "triangulation": {
    "method": 2,
    "zmin": 0.01,
    "zmax": 15.0
  },

  "depth_opt": {
    "two_view": false,
    "use_hessian": true,
    "max_iters": 5,
    "eps": 1e-3,
    "damping": 1e-3,
    "max_res_norm": 2.5 // maximal norm of per observation residuals
  },

  // "feature_P0_damping": 1.0, // 10.0 seems most appropriate

  "imu_calib": {
    "Cas": [1, 1, 1],
    "Car": [[1, 0, 0], 
            [0, 1, 0], 
            [0, 0, 1]],
    "Cgs": [1, 1, 1],
    "Cgr": [[1, 0, 0], 
            [0, 1, 0], 
            [0, 0, 1]]
  },
  "gravity_init_counter": 0,

  "camera_cfg": {
    "model": "radtan",
    "rows": 480, 
    "cols": 752,
    "fx": 458.654,
    "fy": 457.296,
    "cx": 367.215,
    "cy": 248.375,
    "k012": [ 0.00019359, 1.76187114e-05, 0.00 ],
    "p1": -0.28340811,
    "p2": 0.07395907,
    "max_iter": 15
  },

  "min_inliers": 5, // minimum number of inlier measurements

  "MH_thresh": 8.991, // 8.991
  "MH_adjust_factor": 2.0,

  "1pt_RANSAC_thresh": 1.5,
  "1pt_RANSAC_prob": 0.95,
  "1pt_RANSAC_Chi2": 5.89,

  "tracker_cfg": {
    "use_prediction": false,
    "mask_size": 15,
    "margin": 8, // image boundary to mask out
    "num_features_min": 60,
    "num_features_max": 120,
    "max_pixel_displacement": 64,
    "normalize": false,
    "match_dropped_tracks": true,

    "KLT": {
      "win_size": 15,
      "max_level": 5,
      "max_iter": 30,
      "eps": 0.01
    },

    "extract_descriptor": true,
    "descriptor_distance_thresh": -1, // -1 to disable descriptor check
    "default_descriptor": "BRIEF",

    // "detector": "BRISK",
    "detector": "FAST",
    // "detector": "ORB",
    // "detector": "AGAST",
    // "detector": "GFTT",

    "FAST": {
      // https://docs.opencv.org/3.4/df/d74/classcv_1_1FastFeatureDetector.html
      "threshold": 20,
      "nonmaxSuppression": true
    },

    "BRISK": {
      // https://docs.opencv.org/3.4/de/dbf/classcv_1_1BRISK.html
      "thresh": 25,
      "octaves": 3,
      "patternScale": 1.0
    },

    "ORB": {
      // https://docs.opencv.org/3.3.0/db/d95/classcv_1_1ORB.html
      "nfeatures": 500,
      "scaleFactor": 1.2,
      "nlevels": 4,
      "edgeThreshold": 31,
      "firstLevel": 0,
      "WTA_K": 2,
      "patchSize": 31,
      "fastThreshold": 20
    },

    "AGAST": {
      // https://docs.opencv.org/3.4/d7/d19/classcv_1_1AgastFeatureDetector.html
      "threshold": 10,
      "nonmaxSuppression": true
    },

    "GFTT": {
      // Good Feature To Track 
      // https://docs.opencv.org/3.3.0/df/d21/classcv_1_1GFTTDetector.html
      "maxCorners": 1000,
      "qualityLevel": 0.01,
      "minDistance": 1.0,
      "blockSize": 3,
      "useHarrisDetector": false,
      "k": 0.04
    },

    "SIFT": {
      // SIFT
      // https://docs.opencv.org/3.4/d7/d60/classcv_1_1SIFT.html
      "nfeatures": 0,
      "nOctaveLayers": 2,
      "contrastThreshold": 0.000,
      "edgeThreshold": 500.0,
      "sigma": 1.6
    },

    "SURF": {
      // SURF
      // https://docs.opencv.org/3.4/d5/df7/classcv_1_1xfeatures2d_1_1SURF.html 
      "hessianThreshold": 100,
      "nOctaves": 1,
      "nOctaveLayers": 2,
      "extended": false,
      "upright": false
    },

    "BRIEF": {
      "bytes": 32,
      "use_orientation": false
    },

    "FREAK": {
      "orientationNormalized": true,
      "scaleNormalized": true,
      "patternScale": 22.0,
      "nOctaves": 4
    }
  },

  "mapper_cfg": {
    "detectLoopClosures": true,
    "vocabulary": "cfg/ukbench10K_FASTBRIEF32.yml.gz",
    "uplevel_word_search": 0,
    "nn_dist_thresh": 20.0,
    "merge_features": true,
    "feature_merge_cov_factor": 1.0,
    
    "RANSAC": {
      "max_iter": 1000,
      "min_iter": 100,
      "threshold": 0.03, // In units of px/focal_length
      "probability": 0.95,
      "early_exit": false,
      "early_exit_min_iter": 5,
      "early_exit_inlier_ratio": 0.85
    }
  }
}
